{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(auth)/registerScreen.jsx",
    "app/(auth)/loginScreen.jsx",
    "app/(auth)/loginPage.jsx",
    "app/(auth)/forgotPassword.jsx",
    "app/onboarding.jsx",
    "app/index.jsx",
    "app/_layout.jsx"
  ]
}
